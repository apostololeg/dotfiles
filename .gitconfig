[user]
    name = Oleg Apostol
    email = truerenton@gmail.com
[color]
    diff = auto
    status = auto
    branch = auto
[alias]
    co = checkout
    ci = commit -a
    st = status
    b = branch
    l = log --graph --pretty=format:'%C(110)%h%Creset %C(243)[%an]%Creset | %C(250)%s%Creset (%ad, %ar)%C(cyan)%d%Creset' --date=short -5
    log = log --graph --pretty=format:'%C(110)%h%Creset %C(242)[%an]%Creset | %C(250)%s%Creset (%ad)%C(cyan)%d%Creset' --date=local --all
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    rpop = reset --hard HEAD^
[core]
    ignorecase = false
    whitespace = fix,-indent-with-non-tab,trailing-space,space-before-tab
    excludesfile = ~/.gitignore
    editor = vim --wait
[merge]
    log = true
[branch]
    autosetuprebase = always
[push]
    autoSetupRemote = true
    default = current
[url "git@github.com:"]
	insteadOf = https://github.com/
